<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginPage</servlet-name>
  	<display-name>LoginPage</display-name>
  	<description></description>
  	<servlet-class>assignment1.LoginPage</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MainServlet</servlet-name>
  	<display-name>MainServlet</display-name>
  	<description></description>
  	<servlet-class>assignment1.MainServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Prompt3</servlet-name>
  	<display-name>Prompt3</display-name>
  	<description></description>
  	<servlet-class>assignment1.Prompt3</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginPage</servlet-name>
  	<url-pattern>/LoginPage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MainServlet</servlet-name>
  	<url-pattern>/MainServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Prompt3</servlet-name>
  	<url-pattern>/Prompt3</url-pattern>
  </servlet-mapping>
</web-app>
